datasource db {
    provider "postgresql"
    url: env("DATABASE_URL")
}

model Books {
    id integer
    title string
    url string
    cover string
    author Authors
    published_at DateTime
    registered_at DateTime
    categories Categories[]
    num_pages integer
}

model Authors {
    id integer
    name string
    profile string
    about string
    contact string
    registered_at
}

model Publishers {
    id integer
    name string
    homepage string
    origin string
    logo string
    registered_at
}

model Tags {
    name string
    book_id Books
}